{
	"cli": {
		"version": ">=5.0.0"
	},
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal"
		},
		"preview": {
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			},
			"env": {
				"EXPO_PUBLIC_ENV": "preview"
			}
		},
		"production": {
			"distribution": "store",
			"android": {
				"buildType": "app-bundle"
			},
			"env": {
				"EXPO_PUBLIC_ENV": "production"
			}
		}
	}
}
